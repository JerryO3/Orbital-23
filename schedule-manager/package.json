{
  "name": "schedule-manager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "firebase": "^9.22.2",
    "firebase-admin": "^11.9.0",
    "firebase-functions": "^4.4.1",
    "luxon": "^3.3.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.12.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.7.2",
    "url": "^0.11.1",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "tailwind": "npx tailwindcss -i ./src/index.css -o ./src/output.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.22.5",
    "autoprefixer": "^10.4.14",
    "jest": "^27.5.1",
    "jest-cli": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-jsx"
    ],
    "presets": [
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/env"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/(?!${esModules})"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/mocks/fileMocks.js",
      "\\.(css|less)$": "<rootDir>/src/mocks/fileMocks.js"
    }
  }
}
